# https://github.com/ohmyzsh/ohmyzsh/blob/master/lib/directories.zsh

alias ... 'cd ../..'
alias .... 'cd ../../..'
alias ..... 'cd ../../../..'
alias ...... 'cd ../../../../..'

alias md 'mkdir -p'
alias rd rmdir

# List directory contents
alias lsa 'ls -lah'
alias l 'ls -lah'
alias ll 'ls -lh'
alias la 'ls -lAh'
